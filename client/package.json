    "furyroad-interfaces": "^0.1.16",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.4.1",
    "redux-logger": "^3.0.6",
    "typesafe-actions": "^4.4.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.5",
    "@types/redux-logger": "^3.0.7",
    "eslint": "^6.6.0",
    "prettier": "^1.18.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  }
