{
  "name": "furyroad",
  "description": "Fury Road - a post-apocalyptic game.",
  "version": "0.1.5",
  "private": true,
  "proxy": "http://localhost:3030/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" && prettier --write \"src/**/*.tsx\" \"src/**/*.js\"",
    "lint": "tslint --project ./tsconfig.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "furyroad-interfaces": "^0.1.14",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "react-scripts": "^3.1.2",
    "redux-logger": "^3.0.6",
    "typesafe-actions": "^4.4.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/redux-logger": "^3.0.7",
    "eslint": "^6.4.0",
    "prettier": "^1.18.2",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  }
}
